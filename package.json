    "vite": "^2.9.5",
    "vite-plugin-electron": "^0.4.3",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-resolve": "^2.0.4",
    "vue": "^3.2.33",
    "vue-tsc": "^0.34.9"
