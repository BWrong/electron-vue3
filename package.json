    "vue3",
    "vue"
  ],
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "prebuild": "node scripts/build.mjs",
    "build": "electron-builder",
    "tsc": "vue-tsc --noEmit --p packages/renderer/tsconfig.json",
